{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
      "jsx": "react-jsx",
      "esModuleInterop": true, 
      "moduleResolution": "node", 
      "skipLibCheck": true, 
      "strict": true, 
      "noUnusedLocals": true, 
      "noUnusedParameters": true, 
      "noFallthroughCasesInSwitch": true, 
      "noUncheckedSideEffectImports": true 
    },
    "include": ["src"]
  }